{
  "name": "sketch-extract-text",
  "version": "0.5.0",
  "description": "A Sketch plugin to extract text from layers that match a regular expression",
  "keywords": [
    "plugin",
    "sketch",
    "sketch-plugin-helper"
  ],
  "license": "MIT",
  "author": "Lim Yuan Qing",
  "repository": "yuanqing/sketch-extract-text",
  "files": [
    "src"
  ],
  "scripts": {
    "build": "sph build",
    "fix": "sph lint --fix",
    "lint": "sph lint",
    "symlink": "sph symlink",
    "unlink": "sph symlink --delete",
    "version": "sph version",
    "watch": "sph build --watch"
  },
  "devDependencies": {
    "sketch-plugin-helper": "*"
  },
  "sketch-plugin-helper": {
    "name": "Extract Text",
    "menu": [
      {
        "handler": "extract-text",
        "label": "Extract Text"
      },
      "-",
      {
        "handler": "reset-settings",
        "label": "Reset Settings"
      }
    ],
    "defaultSettings": {
      "matchType": "Match layer content",
      "regularExpression": "^Remark"
    }
  }
}
