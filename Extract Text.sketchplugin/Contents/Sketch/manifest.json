{
  "name": "Extract Text",
  "description": "A Sketch plugin to extract text from layers that match a regular expression",
  "author": "Lim Yuan Qing",
  "homepage": "https://github.com/yuanqing/sketch-extract-text",
  "version": "0.2.1",
  "identifier": "yuanqing.sketch-extract-text",
  "appcast": "https://raw.githubusercontent.com/yuanqing/sketch-extract-text/master/appcast.xml",
  "bundleVersion": 1,
  "disableCocoaScriptPreprocessor": true,
  "commands": [
    {
      "name": "Extract Text",
      "identifier": "yuanqing.sketch-extract-text.extract-text",
      "script": "plugin.js",
      "shortcut": "ctrl option cmd e",
      "handler": "extract-text"
    }
  ],
  "menu": {
    "items": [
      "yuanqing.sketch-extract-text.extract-text"
    ],
    "isRoot": true
  }
}
